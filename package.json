{
  "name": "queerkit-com",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "sync": "git submodule update --init --recursive && git -C .rimelight-utilities fetch origin && git -C .rimelight-utilities checkout main && git -C .rimelight-utilities pull origin main && bun ./.rimelight-utilities/scripts/sync.ts && husky && nuxt prepare",
    "analyze": "nuxi analyze",
    "version:sync": "bun run scripts/bump-version.ts",
    "build": "bun run version:sync && nuxi build",
    "build:web": "bun run build",
    "build:android": "bun run version:sync && bun run generate && bun tauri android build --target aarch64 armv7",
    "build:tauri": "NUXT_PUBLIC_API_BASE=https://queerkit.com NUXT_APP_TARGET=tauri bun run generate",
    "dev": "nuxt dev",
    "dev:tauri": "tauri dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt prepare && vue-tsc --noEmit && nuxt typecheck",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "format": "oxfmt --check",
    "format:fix": "oxfmt .",
    "i18n": "vue-i18n-extract report --vueFiles './{app,server,shared}/**/*.?(js|vue|ts)' --languageFiles './i18n/locales/*.?(json|yml|yaml|js)'",
    "check": "bun run typecheck && bun run lint && bun run format",
    "fix": "bun run lint:fix && bun run format:fix",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "tauri": "tauri"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@nuxt/content": "^3.11.2",
    "@nuxt/image": "^2.0.0",
    "@nuxt/scripts": "^0.13.2",
    "@nuxt/ui": "^4.5.0",
    "@nuxtjs/i18n": "^10.2.3",
    "@nuxtjs/robots": "5.7.0",
    "@nuxtjs/sitemap": "^7.6.0",
    "@pinia/colada": "^0.21.2",
    "@pinia/colada-nuxt": "0.3.1",
    "@pinia/nuxt": "0.11.3",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-http": "^2.5.7",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@unhead/vue": "^2.1.2",
    "@vueuse/nuxt": "^14.2.1",
    "better-auth": "^1.4.18",
    "cobe": "^0.6.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "jspdf": "^4.2.0",
    "motion-v": "^2.0.0",
    "nuxt": "^4.3.1",
    "nuxt-og-image": "^5.1.13",
    "pinia": "^3.0.4",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.2.1",
    "uuid": "^13.0.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2",
    "vuedraggable": "^4.1.0",
    "y-protocols": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@iconify-json/lucide": "^1.2.88",
    "@nuxt/devtools": "^3.2.2",
    "@nuxt/a11y": "^1.0.0-alpha.1",
    "@nuxt/schema": "^4.3.1",
    "@nuxt/test-utils": "^3.23.0",
    "@pinia/colada-devtools": "^0.4.2",
    "@tauri-apps/cli": "^2.10.0",
    "@types/chroma-js": "^3.1.2",
    "@types/node": "^25.2.0",
    "drizzle-kit": "^0.31.9",
    "husky": "^9.1.7",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^3.2.5",
    "wrangler": "^4.68.1"
  }
}
